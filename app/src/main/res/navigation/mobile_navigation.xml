<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.ultimate.www.component_application.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.ultimate.www.component_application.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_function"
        android:name="com.ultimate.www.component_application.ui.function.FunctionFragment"
        android:label="@string/title_function"
        tools:layout="@layout/fragment_function" />

    <fragment
        android:id="@+id/navigation_personcenter"
        android:name="com.ultimate.www.component_application.ui.me.MeFragment"
        android:label="@string/title_personcenter"
        tools:layout="@layout/fragment_me" />
</navigation>